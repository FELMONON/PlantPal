{
  "expo": {
    "name": "PlantPal",
    "slug": "plantpal-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "plantpal",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "bundler": "metro",
      "output": "server",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router", 
      "expo-font", 
      "expo-web-browser",
      "expo-camera",
      "expo-image-picker",
      "expo-notifications"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "permissions": [
      "CAMERA",
      "MEDIA_LIBRARY",
      "WRITE_EXTERNAL_STORAGE",
      "NOTIFICATIONS"
    ]
  }
}